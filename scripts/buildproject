mkdir projectBuild
cp -r src projectBuild/src
cp ormconfig.json projectBuild/ormconfig.json
cp package.json projectBuild/package.json
cp package-lock.json projectBuild/package-lock.json
cp tsconfig.json projectBuild/tsconfig.json
cp -r common projectBuild/common

cd client
npm run build
cp index.*.js index.html index.*.css ../projectBuild/src/public